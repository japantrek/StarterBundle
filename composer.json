{
	"name" : "nvbooster/starter-bundle",
	"type" : "symfony-bundle",
	"description" : "starter-bundle",
	"keywords" : [
		"Symfony CMF",
		"starter"
	],
	"homepage" : "https://github.com/nvbooster/StarterBundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Vyacheslav Nechipurenko",
			"email" : "nvb@aproxima.ru",
			"homepage" : "https://github.com/nvbooster"
		}
	],
	"minimum-stability" : "dev",
	"require" : {
        "symfony-cmf/symfony-cmf" : "2.0.*",
        "symfony-cmf/create-bundle" : "2.0.*",
		"helios-ag/fm-elfinder-bundle" : "^6.2",
		"liip/imagine-bundle" : "^1.6",
		"sonata-project/doctrine-phpcr-admin-bundle" : "^2.0",
        "sonata-project/admin-bundle" : "^3.2",
        "sonata-project/translation-bundle" : "^2.0"
	},
	"suggest" : {
		"symfony-cmf/seo-bundle" : "For metadata and sitemaps",
	},
	"autoload" : {
		"psr-4" : {
			"nvbooster\\StarterBundle\\" : ""
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "master"
		}
	}
}