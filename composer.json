{
	"name" : "nvbooster/starter-bundle",
	"type" : "symfony-bundle",
	"description" : "starter-bundle",
	"keywords" : [
		"Symfony CMF",
		"starter"
	],
	"homepage" : "https://github.com/nvbooster/StarterBundle",
	"license" : "MIT",
	"authors" : [{
			"name" : "Vyacheslav Nechipurenko",
			"email" : "nvb@aproxima.ru",
			"homepage" : "https://github.com/nvbooster"
		}
	],
	"minimum-stability" : "dev",
	"require" : {
		"symfony-cmf/block-bundle" : "^2.0",
		"symfony-cmf/content-bundle" : "^2.0",
		"symfony-cmf/routing-bundle" : "^2.0",
		"symfony-cmf/menu-bundle" : "^2.0",
		"sonata-project/doctrine-phpcr-admin-bundle" : "^2.0",
		"sonata-project/translation-bundle" : "^2.0"
	},
	"suggest" : {
		"symfony-cmf/create-bundle" : "For inplace content editing ^2.0",
		"symfony-cmf/seo-bundle" : "For metadata and sitemaps ^2.0"
	},
	"autoload" : {
		"psr-4" : {
			"nvbooster\\StarterBundle\\" : ""
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "master-dev"
		}
	}
}